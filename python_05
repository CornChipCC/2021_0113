# 출처 : https://programmers.co.kr/learn/courses/30/lessons/12935
# Q6 Answer template

def solution(arr):
    min = arr[0]
    index = 0
    for i in range(len(arr)):
        if arr[i] <min :
            min = arr[i]
            index = i
    arr.pop(index)
    if(len(arr) == 0) :
        arr.append(-1)
    return arr

arr = [4, 3, 2, 1]
answer = solution(arr)
print(answer)
