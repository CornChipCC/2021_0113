# 출처 : https://programmers.co.kr/learn/courses/30/lessons/86051
# Q2 Answer template

def solution(numbers):
    answer = 45

    for i in range(len(numbers)):
        num = int(numbers[i])
        answer -= num
        
    return answer

numbers = [1,2,3,4,6,7,8,0]
answer = solution(numbers)
print(answer)
